    "# coding:utf-8\n",
    "import numpy as np\n",
    "\n",
    "# 標準正規分布による乱数を10コ生成する\n",
    "x = np.random.randn(10)\n",
    "print x"
   ]
  },
  {
   "cell_type": "code",
