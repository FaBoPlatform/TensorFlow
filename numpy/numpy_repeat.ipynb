    "# coding:utf-8\n",
    "import numpy as np\n",
    "\n",
    "# 配列の各要素を4つずつ並べる\n",
    "x = np.array([3,6,9]).repeat(4)\n",
    "print x"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
